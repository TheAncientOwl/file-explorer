{
  "name": "file-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "electron": "^15.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "react-scripts": "4.0.3"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm electron:start\"",
    "electron:build": "",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
